        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.2",
        "prettier": "^2.1.2",
        "style-loader": "^2.0.0",
        "stylelint": "^13.0.0",
        "stylelint-config-standard": "^20.0.0",
        "ts-loader": "^8.0.6",
        "typescript": "^4.0.3",
        "webpack": "^5.2.0",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0"
